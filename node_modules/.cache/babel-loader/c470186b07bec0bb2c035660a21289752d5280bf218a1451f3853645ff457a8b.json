      };\r\n    },\r\n    methods: {\r\n      registerUser() {\r\n        // Lógica de registro aqui (por exemplo, enviar dados para o backend)\r\n        // Neste exemplo, apenas exibimos uma mensagem de sucesso\r\n        this.isRegistered = true;\r\n      },\r\n      loginWithGoogle() {\r\n        const provider = new firebase.auth.GoogleAuthProvider();\r\n        \r\n        firebase.auth().signInWithPopup(provider)\r\n          .then((result) => {\r\n            // Lógica de autenticação bem-sucedida\r\n            console.log(result.user);\r\n          })\r\n          .catch((error) => {\r\n            // Tratar erros de autenticação\r\n            console.error(error);\r\n          });\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  "],"mappings":"AAgBA,OAAAA,QAAA;AACA;AAEA;EACAC,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACAC,QAAA;MACAC,KAAA;MACAC,QAAA;MACAC,YAAA;IACA;EACA;EACAC,OAAA;IACAC,YAAA,WAAAA,aAAA;MACA;MACA;MACA,KAAAF,YAAA;IACA;IACAG,eAAA,WAAAA,gBAAA;MACA,IAAAC,QAAA,OAAAV,QAAA,CAAAW,IAAA,CAAAC,kBAAA;MAEAZ,QAAA,CAAAW,IAAA,GAAAE,eAAA,CAAAH,QAAA,EACAI,IAAA,WAAAC,MAAA;QACA;QACAC,OAAA,CAAAC,GAAA,CAAAF,MAAA,CAAAG,IAAA;MACA,GACAC,KAAA,WAAAC,KAAA;QACA;QACAJ,OAAA,CAAAI,KAAA,CAAAA,KAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}